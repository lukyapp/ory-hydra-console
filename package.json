{
  "name": "ory-hydra-console",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "prisma migrate deploy && next start",
    "lint": "eslint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prettier": "prettier --check .",
    "postinstall": "prisma generate",
    "prisma:init": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate",
    "prisma:seed": "npx prisma db seed",
    "dev:postgres": "docker run -d --name ory-hydra-console-postgres-db -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=postgres -p 5435:5432 -v ./volume:/var/lib/postgresql/data postgres:16"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lodash.set": "^4.3.2",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "next-intl": "^4.8.1",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "^7.3.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/lodash.set": "latest",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
